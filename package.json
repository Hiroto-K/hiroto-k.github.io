    "autoprefixer": "10.2.6",
    "axios": "0.21.1",
    "babel-eslint": "10.1.0",
    "cssnano": "5.0.6",
    "eslint": "7.31.0",
    "eslint-config-standard": "16.0.3",
    "eslint-plugin-import": "2.23.4",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "5.1.0",
    "eslint-plugin-standard": "4.1.0",
    "http-server": "0.12.3",
